@model List<SoporteTecnico.BL.Producto>
@{
    ViewBag.Title = "Index";
}


<h2><font color="Navy">NUESTROS SERVICIOS</font></h2>


<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>
                Codigo
            </th>
        
                <th>
                Descripcion
            </th>
        
            <th>
                Precio
            </th>
        </tr>


    </thead>
    @foreach (var producto in Model)
    {
        <tr>
            <td>@producto.Id</td>
            <td>@producto.Descripcion</td>
            <td>@producto.Precio</td>
        </tr>

    }
</table>